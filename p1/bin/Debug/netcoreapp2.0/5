function fun(){
	if( location.href.indexOf("cf.pku") >=0  ) return;
	var w= window.open("about:blank","notice","HEIGHT=200,WIDTH=200" );
	w.document.write("新的校内服务器开通了，请大家访问<a target=_blank onclick=close() href=http://cf.pku.cn/tds>cf.pku.cn/tds</a>");
	//w.document.write("通知：12月26日的计算机课不讲课，同学们自学、复习。但本周照常上机。请同学们互相转告。");
}


var x = new Array();
var i = 0;

function whichweek()
{
	var now = new Date();
	//now = new Date("2003/12/24")
	for( i=0; i<x.length-1; i++ )
		if( now>=x[i] && now<x[i+1]) return i;
	return 0;
}

function scrol(lnk){
	var w = whichweek();
	if( w>0 ) w--; // make it a little up
	lnk.href="#w" + w;
	return false;
}

function openlink(lnk, w)
{
	if( w <= whichweek() )return true; 
	lnk.href="#";
	alert("Sorry,文件暂时无法打开。因为只能打开本周的媒体文件。")
	return false;
}

function wk(){
	var now = new Date();
	//alert(now);
	var first = new Date(2013,1,25); //注意月份
	//alert(first);
	return parseInt((now-first)/24/3600/1000/7);	
}

$(function(){

	var w = wk();
	var a=$("a");
	a.each(function(){ 
		var name=$(this).attr("name");
		
		if(name.substring(0,1)!="w") return;
		
		var idx = parseInt(name.substring(1));
		//alert(idx);
		if( idx!=w+1) return;
		
		var td = $(this).parent().next().next().next();
		//alert(td.html());
		//alert(tr.html());//filter("td:first-child")
		//td.css("class","nodisplay");
		
		//td.html("&nbsp;");
		td.parent().css("background-color","#ffddee");

	});

});
